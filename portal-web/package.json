    "eslint-plugin-react-hooks": "2.5.1",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "stylelint": "13.2.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.17.1",
    "ts-jest": "25.3.0",
    "typescript-plugin-css-modules": "2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:qa": "env-cmd -f .env.qa react-scripts build",
    "test-dev": "react-scripts test",
    "test-verbose": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint-fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.scss'",
    "audit-portal": "audit-ci --critical --allowlist [1486,1548]  --json > portalAudit.html",
    "test": "cross-env CI=true react-scripts test",
    "coverage": "yarn test -- --coverage",
    "prettier-format": "prettier --write '**/*.{ts,js,tsx,css,scss}'",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/**/index.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".*__snapshots__/.*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint './src/**/*.{ts,tsx}' --fix",
      "yarn test --bail --findRelatedTests",
      "yarn audit-portal"
    ],
    "src/**/*.{scss,css}": [
      "stylelint --config=.stylelintrc.json --fix",
      "prettier --config .prettierrc --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
